@page
@using ToDo.Models
@model IndexModel
@{
    ViewData["Title"] = "Home page";
}

<div>
    @foreach (ToDoTask item in Model.ToDoTasks)
    {
        <div>
            <p>@item.Title</p>
            <p>@item.Description</p>
            <p>@item.TaskPriority</p>
            <p>@item.Created</p>
        </div>
    }
</div>